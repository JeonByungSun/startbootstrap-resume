































pipeline {
    agent any
    environment {
        DEPLOY_PATH = "/var/www/html/dist"
    }
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', 
                    credentialsId: 'github-credentials', 
                    url: 'https://github.com/JeonByungSun/startbootstrap-resume.git'  # ✅ 내 GitHub 계정으로 수정!
            }
        }
    }
}

